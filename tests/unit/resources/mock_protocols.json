            "protocol_core": {
                "protocol_id": "Singapore_dissociation_protocol",
                "protocol_name": "Singapore protocol",
                "protocol_description": "The protocol consisted of tissue digestion by collagenase IV and DNases I in RPMI1640 media at 37°C for 1 h, followed by mechanical shearing with a 16 1/2 gauge needle, washing in PBS containing 1% BSA and 2 mM EDTA, and red blood cell lysis using ACK lysis buffer."
            },
            "method": {
                "text": "enzymatic dissociation",
                "ontology": "EFO:0009128",
                "ontology_label": "enzymatic dissociation"
            }
